<div class="container">
  <h1>Alpha Bloggers</h1>
  <table class="table">
    <thead>
      <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Author</th>
        <th colspan="2">Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= link_to user.username, user %></td>
          
      <% end %>
    </tbody>
  </table>

  <%= link_to "Back to All Articles", users_path, class: "btn btn-info" %>
</div>
